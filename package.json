{
  "name": "flowtly-connect",
  "version": "1.0.0",
  "description": "n8n community node for Flowtly integration",
  "keywords": [
    "n8n-community-node-package",
    "flowtly"
  ],
  "license": "MIT",
  "homepage": "https://app.flowtly.com",
  "author": {
    "name": "Flowtly",
    "email": "contact@flowtly.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowtly/n8n-nodes-flowtly.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FlowtlyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Flowtly/Flowtly.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^0.6.3",
    "gulp-replace": "^1.1.3",
    "gulp-svg-sprite": "^2.0.2",
    "gulp-svgmin": "^4.0.0",
    "n8n-core": "~0.125.0",
    "n8n-workflow": "~0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  }
} 